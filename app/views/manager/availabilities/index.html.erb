<div class="dashboard-wrap">
  <%= render "/shared/sidebar"%>

  <div class="dashboard-content">
    <div>

        <% @techs.each do |tech| %>
          <div class="col-xs-12 col-md-6">
            <div id="events_calendar">
              <%= render partial: "shared/calendar", locals: {availabilities: @availabilities.of(tech), user: @user, tech: tech} %>
            </div>
          </div>
        <% end %>

    </div>

  </div>
</div>
