<div class="dashboard-wrap">
  <%= render "/shared/sidebar"%>

  <div class="dashboard-content">
    <div class="padding-box">
      <div class="row">
        <% @techs.each do |tech| %>
          <div class="col-xs-12 col-md-6">
            <h3>Emploi du temps de <%= tech.first_name %></h3>
            <%= render "shared/calendar", availabilities: @availabilities.of(tech), user: @user, tech: tech %>
          </div>
        <% end %>
      </div>
    </div>

  </div>
</div>
