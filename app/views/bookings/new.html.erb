<% @availabilities.each do |availability| %>
  <p><%= availability %></p>
  <%= simple_form_for :query, method: :get do |f| %>
    <%= f.input :status, as: :boolean %>
  <% end %>
<% end %>
    <%= f.button :submit %>

<%= simple_form_for @booking, @availabilities do |f| %>
  <%= f.input :address %>
  <%= f.input :surface %>
  <%= f.input :comment %>
  <%= f.button :submit %>
<% end %>
